generator client {
    provider = "prisma-client-js"
}

datasource db {
    // provider = "sqlite"
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Records {
    id        String   @id @default(uuid())
    sensorId  String
    amount    Int
    smoke     Boolean
    createdAt DateTime @default(now())

